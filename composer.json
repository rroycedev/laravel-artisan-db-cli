{
    "name": "rroycedev/laravel-artisan-db-cli",
    "description": "Laravel artisan CLI to maintain database migrations",
    "type": "project",
    "require": {
        "laravel/framework": "^5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ron Royce",
            "email": "rroycedev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Roycedev\\Roycedb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roycedev\\DbCli\\DbCliServiceProvider",
            ],
            "aliases": {
                "Roycedb": "Roycedev\\Roycedb\\Facades\\Roycedb"
            }
        }
    }
}
